<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="add" class="com.entity.Address">

		<property name="aid" value="211"></property>
		<property name="address" value="Pune"></property>

	</bean>

	<bean id="stud" class="com.entity.Student">

		<property name="sid" value="511"></property>
		<property name="sname" value="Sudarshan"></property>
		<property name="address" ref="add"></property>

	</bean>
	
</beans>