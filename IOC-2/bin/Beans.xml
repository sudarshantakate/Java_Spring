<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="stud" class="com.entity.Student">
		<property name="sid" value="121"></property>
		<property name="sname" value="Sudarshan"></property>
		<property name="listofcourse">
			<list>
				<value>Java</value>
				<value>Python</value>
				<value>Cpp</value>
			</list>
		</property>

		<property name="listofrollnumbers">
			<list>
				<value>6201</value>
				<value>6202</value>
			</list>
		</property>

		<property name="listofstudentmarks">
			<map>
				<entry key="Java" value="90"></entry>
				<entry key="Python" value="95"></entry>
			</map>
		</property>
	</bean>

</beans>
